{
    "name": "iDeleteV2",
    "version": "2.0",
    "description": "Delete all images in a website",
    "manifest_version": 2,
 

    "content_scripts" : [{
        "matches": ["<all_urls>"],
        "js":["content.js"]
    }],

    "background":{
        "scripts":["background.js"]
    },

    "browser_action": {
        "default_icon":"icondel.png"
    }

}